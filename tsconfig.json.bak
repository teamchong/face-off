{
  "compilerOptions": {
    "outDir": "build/dist",
    "module": "esnext",
    "target": "es5",
    "lib": ["es6", "dom"],
    "sourceMap": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "src",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": "./packages",
    "paths": {
      "@material-ui/core": ["./material-ui/src"],
      "@material-ui/core/*": ["./material-ui/src/*"],
      "@material-ui/lab": ["./material-ui-lab/src"],
      "@material-ui/lab/*": ["./material-ui-lab/src/*"]
    }
  },
  "exclude": [
    "node_modules",
    "build",
    "scripts",
    "acceptance-tests",
    "webpack",
    "jest",
    "src/setupTests.ts"
  ]
}
